<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="400.0" prefWidth="600.0"
            styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8"
            fx:controller="setlab.controller.FeedbackWindowController">
    <stylesheets>
        <URL value="@/setlab/css/feedbackwindow.css"/>
    </stylesheets>
    <children>
        <TextField fx:id="fd_titleField" focusTraversable="false" layoutX="14.0" layoutY="24.0" prefHeight="31.0"
                   prefWidth="520.0" promptText="Напишите кратко про ошибку или идею" AnchorPane.leftAnchor="6.0"
                   AnchorPane.rightAnchor="70.0" AnchorPane.topAnchor="26.0"/>
        <TextArea fx:id="fd_textArea" focusTraversable="false" layoutX="14.0" layoutY="62.0" prefHeight="200.0"
                  prefWidth="200.0"
                  promptText="Здесь вы можете описать вашу проблему и оставить адрес для ответа (не обезательно)"
                  AnchorPane.bottomAnchor="44.0" AnchorPane.leftAnchor="6.0" AnchorPane.rightAnchor="6.0"
                  AnchorPane.topAnchor="64.0"/>
        <Button fx:id="fd_btnSend" layoutX="14.0" layoutY="355.0" mnemonicParsing="false" prefHeight="32.0"
                prefWidth="100.0" text="Отправить" AnchorPane.bottomAnchor="6.0" AnchorPane.leftAnchor="6.0"/>
        <Label fx:id="fd_score" layoutX="551.0" layoutY="29.0" prefHeight="21.0" prefWidth="60.0" text="100/100"
               AnchorPane.rightAnchor="6.0" AnchorPane.topAnchor="30.0"/>
        <Label fx:id="fd_score1" layoutX="14.0" layoutY="4.0" text="Тема" AnchorPane.leftAnchor="6.0"
               AnchorPane.topAnchor="6.0"/>
    </children>
</AnchorPane>
